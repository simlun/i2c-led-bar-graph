/*******************************************************************************
 * I2C
 ******************************************************************************/

I2C <- hardware.i2c12


function configureI2C() {
  I2C.configure(CLOCK_SPEED_100_KHZ)
}



/*******************************************************************************
 * Main
 ******************************************************************************/

function main() {
  configureI2C()
  setupExpander()
  expanderBlinky()
}


main()
